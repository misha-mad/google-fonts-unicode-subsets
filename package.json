{
  "name": "google-fonts-unicode-subsets",
  "version": "0.1.0",
  "description": "Generator and dataset of Unicode subsets for Google Fonts, built from official .nam files.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rm -rf dist",
    "generate": "tsx src/generate.ts && prettier --write ./src/google-fonts-subsets*.json",
    "test": "vitest run",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "devDependencies": {
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.3.0",
    "@types/node": "24.10.1",
    "prettier": "3.7.2",
    "rollup": "4.53.3",
    "rollup-plugin-dts": "6.3.0",
    "typescript": "5.9.3",
    "vitest": "4.0.15"
  }
}
